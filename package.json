{
  "name": "lis-frontend",
  "version": "0.0.1",
  "scripts": {
    "dev": "next dev",
    "build:production": "cp .env.production .env.production.local && next build",
    "build:development": "cp .env.development .env.production.local && next build",
    "build:staging": "cp .env.staging .env.production.local && next build",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build:development",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "check-types": "tsc --noEmit --pretty",
    "test": "vitest",
    "test:synpress": "synpress run --configFile synpress.config.js",
    "open": "synpress open --configFile synpress.config.js",
    "commit": "git-cz",
    "postbuild": "next-sitemap",
    "ac": "npm run check-types && npm run lint:fix && git add . && npm run commit",
    "codegen": "node codegen/codegen.js",
    "test:jest": "jest"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@mantine/carousel": "^7.11.0",
    "@mantine/core": "^7.11.0",
    "@mantine/dates": "^7.11.0",
    "@mantine/hooks": "^7.11.0",
    "@next/bundle-analyzer": "^13.4.19",
    "@react-pdf/renderer": "^3.4.4",
    "@tanstack/react-query": "^5.45.0",
    "async-mutex": "^0.4.0",
    "axios": "^1.6.2",
    "chart.js": "^4.4.1",
    "clsx": "^2.0.0",
    "embla-carousel-react": "^7.1.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^10.16.16",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "next": "^14.0.3",
    "next-seo": "^6.4.0",
    "next-sitemap": "^4.2.3",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-pdf": "^9.0.0",
    "sharp": "^0.33.0",
    "socket.io-client": "^4.7.5",
    "tailwind-merge": "^2.1.0",
    "viem": "^2.13.7",
    "vite-tsconfig-paths": "^4.3.2",
    "wagmi": "^2.9.10",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/cz-commitlint": "^17.7.1",
    "@svgr/webpack": "^8.1.0",
    "@synthetixio/synpress": "2.4.1-beta.2",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/file-saver": "^2.0.7",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.15",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "cypress": "^12.17.4",
    "cz-conventional-changelog": "^3.3.0",
    "cz-git": "^1.7.1",
    "eslint": "^8.48.0",
    "eslint-config-airbnb": "^19.0.4",
