    "sass": "^1.66.1",
    "start-server-and-test": "^2.0.0",
    "swagger-typescript-api": "^13.0.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite-plugin-magical-svg": "^1.2.1",
    "vitest": "^1.6.0"
